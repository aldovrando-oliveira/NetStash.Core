<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.0.0</Version>

    <PackageId>NetStash.Core</PackageId>
    <Authors>Aldo Oliveira</Authors>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseUrl>https://github.com/aldovrando-oliveira/NetStash.Core/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/aldovrando-oliveira/NetStash.Core</PackageProjectUrl>
    <PackageTags>Logger, Logstash, Tcp</PackageTags>
    <RepositoryUrl>https://github.com/aldovrando-oliveira/NetStash.Core</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Data.Sqlite" Version="2.1.0" />
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
  </ItemGroup>

</Project>
